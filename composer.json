{
    "name": "verbb/stamped-io",
    "description": "Send your order data from Craft Commerce to Stamped.io, for your customers to review.",
    "type": "craft-plugin",
    "version": "2.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "stamped-io"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/stamped-io/issues?state=open",
        "source": "https://github.com/verbb/stamped-io",
        "docs": "https://github.com/verbb/stamped-io",
        "rss": "https://github.com/verbb/stamped-io/commits/v2.atom"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0-alpha",
        "verbb/base": "^2.0.0-beta"
    },
    "autoload": {
        "psr-4": {
          "verbb\\stamped\\": "src/"
        }
    },
    "extra": {
        "name": "Stamped",
        "handle": "stamped-io",
        "schemaVersion": "1.0.0",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/stamped-io/craft-4/CHANGELOG.md",
        "class": "verbb\\stamped\\Stamped"
    }
}
